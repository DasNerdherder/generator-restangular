{
    "name": "<%= packageName %>",
    "version": "0.0.0",
    "private": true,
    "dependencies": {
        "angular": "<%= angularVersion %>",
        "angular-route": "<%= angularVersion %>",
        "restangular": "<%= restangularVersion %>",
        "jquery": "<%= jqueryVersion %>"
    },
    "devDependencies": {
        "angular-mocks": "<%= angularVersion %>",
        "jasmine-jquery": "1.7.0",
        "jasmine": "1.3.1"
    },
    "exportsOverride": {
        "angular": {
            "live": "angular.min.js",
            "dev": "angular.js"
        },
        "angular-route": {
            "live": "angular-route.min.js",
            "dev": "angular-route.js"
        },
        "restangular": {
            "live": "dist/restangular.min.js",
            "dev": "dist/restangular.js"
        },
        "lodash": {
            "live": "dist/lodash.min.js",
            "dev": "dist/lodash.js"
        },
        "jquery": {
            "live": "jquery.min.js",
            "dev": "jquery.js"
        }
    }
}

